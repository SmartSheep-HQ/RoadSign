{
  "name": "Example Site",
  "rules": [
    {
      "host": [
        "localhost"
      ],
      "path": [
        "/site1"
      ]
    }
  ],
  "upstreams": [
    {
      "name": "Example Upstream",
      "uri": "file:///site"
    }
  ],
  "transformers": [
    {
      "type": "replacePath",
      "options": {
        "pattern": "/site1",
        "value": "/"
      }
    }
  ]
}